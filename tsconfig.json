{
  "compilerOptions": {
    "noEmit": true,
    "module": "esnext",
    "target": "ES2021",
    "moduleResolution": "node",
    "jsx": "preserve",
    "strict": true,
    "noImplicitAny": true,
    "checkJs": true,
    "allowJs": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/FrameMafs.tsx", "components/MultiplyChoice.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
